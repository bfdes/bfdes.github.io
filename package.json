    "morgan": "^1.9.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/express": "^4.11.1",
    "@types/highlight.js": "^9.12.2",
    "@types/jest": "^22.2.2",
    "@types/katex": "^0.5.0",
    "@types/marked": "^0.3.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-router-dom": "^4.2.3",
    "@types/supertest": "^2.0.4",
